{
    "presets": [
        [
            "@babel/env",
            {
                "esmodules": false,
                "targets": {
                    "browsers": ["> 3%"]
                }
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-transform-classes"],
        ["@babel/plugin-transform-runtime", { "polyfill": false }]
    ]
}