{
    "presets": [
        [
            "@babel/preset-env",
            {
                "esmodules": false,
                "targets": {
                    "browsers": ["> 3%"]
                }
            }
        ],
        ["@babel/preset-react"]
    ],
    "plugins": [
        ["@babel/plugin-transform-classes"],
        ["@babel/plugin-transform-runtime"]
    ]
}